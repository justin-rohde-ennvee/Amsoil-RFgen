Private Sub ReleasePick()
  Dim sSQL As String

  ' Release any suggestions that have not been picked
  sSQL = _
    " update WDJOBS" & _
    " set R2URRF = ' '" & _
    " ,   R2TRIP = 0" & _
    " ,   R2PSTB = '320'" & _
    " ,   R2DSC2 = ' '" & _
    " ,   R2URCD = ' '" & _
    " where R2URRF = '" & UCase(Trim(App.User)) & "'" & _
    " and R2PSTB in ('321')"

'    " ,   R2LPID = ' '" & _

  DB.UseDataSource("RFgenCustom")
  DB.Execute(sSQL)
  DB.UseDataSource(0)

  sSQL = _
    " update WDJOBS" & _
    " set R2URRF = ' '" & _
    " where R2URRF = '" & UCase(Trim(App.User)) & "'" & _
    " and R2PSTB in ('322')"
  DB.UseDataSource("RFgenCustom")
  DB.Execute(sSQL)
  DB.UseDataSource(0)

  lblItem.Caption = ""
  lblLot.Caption = ""
  lblLocn.Caption = ""
  lblQty.Caption = ""
  lblBox.Caption = ""
  lblConfUOM.Caption = ""

  App.SetValue("tmTRIP", 0)
End Sub

