Private Sub txtWO_LostFocus()
  On Error Resume Next

  Dim sSql As String
  Dim sCols As String
  Dim sRows As String
  Dim i As Integer
  Dim sItemNumber As String
  Dim sItemDesc As String
  Dim sAdd As String

  If (Len(App.GetValue("tmDOCO"))=0 Or bPartsValidated) Then ' handles error thrown when query executes with empty tmDOCO
    Exit Sub
  End If

  sSql = "select WMCPIL, WMDSC1, WMCPIA from F3111 where WMDOCO = %tmDOCO"
  DB.Execute(sSql, sCols, sRows)
  If(DB.Count(sRows)=0) Then
    'throw error? if no results
    Exit Sub
  End If

  ReDim currentItems(DB.Count(sRows))
  ReDim currentLitms(DB.Count(sRows))
  ReDim doneItems(1)
  ReDim doneLitms(1)

  For i = 1 To DB.Count(sRows) 'fills List.Data box
    sItemNumber = DB.Extract(sCols, sRows, i , "WMCPIL")
    currentItems(i) = Trim(DB.Extract(sCols, sRows, i , "WMCPIA"))
    currentLitms(i) = Trim(sItemNumber)
    sItemDesc = DB.Extract(sCols, sRows, i , "WMDSC1")
    sAdd = FixLeft(sItemNumber, 13,"")
    sAdd = sAdd + "     " + FixLeft(sItemDesc, 15," ")
    lbxITEM.List.AddItem(sAdd)
  Next

  lblItemCount.Caption = UBound(currentItems) 'caption to display amount of rows


End Sub

