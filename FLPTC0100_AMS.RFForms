Private Sub txtITEM_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next

  Dim sSql As String
  Dim i As Integer
  Dim j As Integer
  Dim itemNum As String
  Dim litmNum As String
  Dim currentSpotTemp As Integer
  Dim itemFound As Boolean
  Cancel = True
  itemFound = False
  Rsp = UCase(Trim(Rsp))
  If Len(Rsp) = 0 Then
    Exit Sub
  End If

  For j = 1 To UBound(doneItems) 'check to see if Item has already been scanned
    If Rsp = doneItems(j) Or Rsp = doneLitms(j) Then
      App.MsgBox("Item Already Scanned")
      Exit Sub
    End If
  Next

  For i = 1 To UBound(currentItems)
    If Rsp = currentItems(i) Or Rsp = currentLitms(i) Then ' if input matches an item on the List.Data
      itemFound = True
      itemNum = currentItems(i)
      litmNum = currentLitms(i)
      doneItems(UBound(doneItems)) = itemNum
      doneLitms(UBound(doneLitms)) = litmNum

      'deletes entered item from currentItems and places item in doneItems(above)
      ReDim tempArray(UBound(currentItems)-1) As String
      currentSpotTemp = 1
      For j = 1 To UBound(currentItems)
        If (currentItems(j) <> itemNum) And (currentLitms(j) <> litmNum)  Then
          tempArray(currentSpotTemp) = currentItems(j)
          currentSpotTemp = currentSpotTemp + 1
        End If
      Next
      currentItems = tempArray

      'deletes litm from current litm's
      currentSpotTemp = 1
      For j = 1 To UBound(currentLitms)
        If (currentItems(j) <> itemNum) And (currentLitms(j) <> litmNum) Then
          tempArray(currentSpotTemp) = currentLitms(j)
          currentSpotTemp = currentSpotTemp + 1
        End If
      Next
      currentLitms = tempArray

      lblItemCount.Caption = UBound(currentItems)
      lbxITEM.List.RemoveItem(i)
      ReDim Preserve doneItems(UBound(doneItems)+1)
      ReDim Preserve doneLitms(UBound(doneLitms)+1)
      Exit For
    End If
  Next

  If Not itemFound Then
    App.MsgBox("Invalid Item")
  End If

  If (UBound(currentItems) = 0) Then

    sSql = "UPDATE F4801 SET WAWR06 = '1' WHERE WADOCO = %tmDOCO and WADCTO ='WO'"
    DB.Execute(sSql)
    lbxITEM.Caption = "Parts List Validated"
    txtITEM.Text = ""
    Cancel = False
  End If
End Sub

