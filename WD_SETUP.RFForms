Private Sub Form_Load()
  On Error Resume Next

  Dim sDRDL01 As String
  Dim sDRDL02 As String
  Dim sUDC As String
  Dim vArray  As Variant
  Dim i As Integer
  Dim sMCU As String
  Dim sBranchPlant As String
  Dim sCompany As String
  Dim sDesc As String

  Call SetDisplay()

  '
  ' Initialize special controls
  '
  lblPutAway.Caption = ""
  lblPutAway.Label.BackColor1 = cFieldDefaultBC
  lblPutAway.Label.ForeColor = cFieldDefaultFC
  '
  lblPick.Caption = ""
  lblPick.Label.BackColor1 = cFieldDefaultBC
  lblPick.Label.ForeColor = cFieldDefaultFC
  '
  lblRepl.Caption = ""
  lblRepl.Label.BackColor1 = cFieldDefaultBC
  lblRepl.Label.ForeColor = cFieldDefaultFC
  '
  lblSave.Caption = ""
  lblSave.Label.BackColor1 = cFieldDefaultBC
  lblSave.Label.ForeColor = cFieldDefaultFC

  Call SetChkBoxes

  vArray = Split(SYS.GetProperty("WD", "MCU"), ";")

  For i = 0 To UBound(vArray)
    sMCU = vArray(i)
    If Validate_BranchPlant(sMCU, sBranchPlant, sCompany, sDesc) Then lbxMCU.List.AddItem(sMCU, sMCU, sDesc)
  Next

  ' Put
  txtPrioPut.text  = SYS.GetProperty("WD", "PrioPut")
  txtZonePut.text  = SYS.GetProperty("WD", "ZonePut")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZonePut.text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblPutAway.Caption = sDRDL01

  ' Pick
  txtPrioPick.text  = SYS.GetProperty("WD", "PrioPick")
  txtZonePick.text = SYS.GetProperty("WD", "ZonePick")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZonePick.text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblPick.Caption = sDRDL01
  txtStatusF.text  = SYS.GetProperty("WD", "StatusF")
  txtStatusT.text  = SYS.GetProperty("WD", "StatusT")

  ' Repl.
  txtPrioRepl.text  = SYS.GetProperty("WD", "PrioRepl")
  txtZoneRepl.text = SYS.GetProperty("WD", "ZoneRepl")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZoneRepl.text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblRepl.Caption = sDRDL01
End Sub

Private Sub txtMCU_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next

  Dim sCompany As String
  Dim sMCU     As String
  Dim sVal     As String
  Dim sDsc     As String
  Dim sDsp     As String
  Dim i       As Long
  Dim nLine   As Integer
  Dim vArray  As Variant
  Dim nCount  As Long
  Dim bDel    As Boolean

  If (Len(Trim(Rsp))=0) Then Exit Sub
  Cancel = True

  If Not Validate_BranchPlant(Rsp, sMCU, sCompany, sDsc) Then Exit Sub

  App.SetValue("tmCO", sCompany)
  App.SetValue("tmMCU", sMCU)

  ' Remove item if already in list; otherwise add it
  nCount = lbxMCU.List.Count
  bDel = False

  For i = 1 To nCount
    sVal = lbxMCU.List.Cell(i, 1).value

    If Trim(sVal) = Trim(Rsp) Then
      lbxMCU.List.RemoveItem(i)
      bDel = True
      Exit For
    End If
  Next

  If Not bDel Then lbxMCU.List.AddItem(sMCU, sMCU, sDsc)
  lbxMCU.List.Sorted = True
  Screen.Refresh
End Sub

