  lblDesc1.Caption    = DB.Extract(sCols,sRows,1,"FADL01")
  lblDesc2.Caption    = DB.Extract(sCols,sRows,1,"FADL02")
  lblSerial.Caption   = DB.Extract(sCols,sRows,1,"FAASID")
  txtAssetNumber.Text = DB.Extract(sCols,sRows,1,"FANUMB")
  lblQty.Caption      = " Qty: " & ConvDecimalsFromSQL("ARCQ",DB.Extract(sCols,sRows,1,"FAARCQ"))
  '
  App.SetValue("AssetNumber", DB.Extract(sCols,sRows,1,"FANUMB"))
  App.SetValue("Quantity",    ConvDecimalsFromSQL("ARCQ",DB.Extract(sCols,sRows,1,"FAARCQ")))
  App.SetValue("Status",      DB.Extract(sCols,sRows,1,"FAEQST"))
  App.SetValue("DateDsp",     DB.Extract(sCols,sRows,1,"FADSP"))
  '
  If App.GetValue("DateDsp") <> 0 Then
    vsDate = App.GetValue("DateDsp")
    ConvJDEtoDate(vsDate)
    App.MsgBox(GetMsg(95) & " " & vsDate)
    Exit Sub
  End If

  Cancel = False
  Rsp = Trim(Rsp)
End Sub

