Private Sub txtAssetNumber_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next
  '
  Dim sSQL  As String
  Dim sCols As String
  Dim sRows As String
  '
  Dim vsDate As String
  '
  If (Len(Rsp) = 0) Then Exit Sub
  Cancel = True
  '
  sSQL = "select FAAPID, FANUMB, FAASID, FADL01, FADL02, FADL03, FAEQST, FAARCQ, FADSP from F1201 " _
       & "where FANUMB = " & Rsp & " and FALOC = '%Location' "
  DB.Execute(sSQL, sCols, sRows)
  '
  If (Len(sRows) = 0) Then
    App.MsgBox(GetMsg(50))
    Exit Sub
  End If
  '
  lblDesc1.Caption    = DB.Extract(sCols,sRows,1,"FADL01")
  lblDesc2.Caption    = DB.Extract(sCols,sRows,1,"FADL02")
  lblSerial.Caption   = DB.Extract(sCols,sRows,1,"FAASID")
  txtUnitNumber.Text  = DB.Extract(sCols,sRows,1,"FAAPID")
  lblQty.Caption      = " Qty: " & ConvDecimalsFromSQL("ARCQ",DB.Extract(sCols,sRows,1,"FAARCQ"))
  '
  App.SetValue("AssetNumber", Rsp)
  App.SetValue("Quantity", ConvDecimalsFromSQL("ARCQ",DB.Extract(sCols,sRows,1,"FAARCQ")))
  App.SetValue("Status",   DB.Extract(sCols,sRows,1,"FAEQST"))
  App.SetValue("DateDsp",  DB.Extract(sCols,sRows,1,"FADSP"))
  '
  If App.GetValue("DateDsp") <> 0 Then
    vsDate = App.GetValue("DateDsp")
    Call ConvJDEtoDate(vsDate)
    App.MsgBox(GetMsg(95) & " " & vsDate)
    Exit Sub
  End If
  '
  Cancel = False
  Rsp = Trim(Rsp)
  '
  txtAssetNumber.Text = Rsp
  App.SetFocus("txtToLocn")
End Sub

