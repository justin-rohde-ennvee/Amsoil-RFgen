Private Sub btnChange_Click()
  On Error Resume Next
  '
  Dim sSQL   As String
  Dim sCols  As String
  Dim sRows  As String
  Dim vArray As Variant
  Dim vRsp   As Variant
  Dim iCnt   As Integer
  Dim nErrNo As Long

  Call GetChkBoxes
  '
  SYS.SetProperty("Log", "Boxes", msChk)
  SetChkBoxes
  '
  msEmail = txtEmail.Text
  SYS.SetProperty("Log", "EmailAddr", msEmail)
  '
  msCC = txtCC.Text
  SYS.SetProperty("Log", "CCAddr", msCC)
  '
  msMailType = txtMailType.Text
  SYS.SetProperty("Log", "EmailType", msMailType)
  '
  msMailServ = txtMailServ.Text
  SYS.SetProperty("Log", "MailServer", msMailServ)
  '
  Call GetScreen

  ' Populate the queue load balancing table
  If chkQLoadBal.Checked Then

    vRsp = App.MsgBox("Replace current records?", vbYesNo, 2)

    If vRsp = vbYes Then
      sSQL = "delete from RFQBAL"
      nErrNo = DB.Execute(sSQL)

      If nErrNo <> 0 Then
        App.MsgBox SysErr.Description
        Exit Sub
      End If
    End If

    vArray = Split(txtMonitorTM.Text, ";")

    For iCnt = 0 To UBound(vArray)
      If Len(Trim(vArray(iCnt))) > 0 Then
        sSQL = "select count(*) from RFQBAL where QNAME = '" & vArray(iCnt) & "'"
        nErrNo = DB.Execute(sSQL, sCols, sRows)

        If nErrNo <> 0 Then
          App.MsgBox SysErr.Description
          Exit Sub
        End If

        If Val(sRows) = 0 Then
          sSQL = "insert into RFQBAL values('" & vArray(iCnt) & "',0)"
          nErrNo = DB.Execute(sSQL)

          If nErrNo <> 0 Then
            App.MsgBox SysErr.Description
            Exit Sub
          End If
        End If
      End If
    Next

