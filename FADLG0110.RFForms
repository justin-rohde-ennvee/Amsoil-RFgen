Public Sub GetData()
  On Error Resume Next
  '
  Dim vArray As Variant
  '
  ' MonitorTM
  '
  vArray = Split(SYS.GetProperty("Config", "MonTM"), "|")
  '
  txtMonitorTM.Text     = vArray(0)
  txtMonitorTMTime.Text = vArray(1)
  txtConnect.Text       = vArray(2)
  '
  ' Environment
  '
  vArray = Split(SYS.GetProperty("Config", "Env"), "|")
  '
  If vArray(0) = "1" Then
    txtJDERelease.Caption   = "OneWorld / Enterprise E1"
    txtSMMain.Visible       = False
    txtSMWarning.Visible    = False
    ChkIsE1.Checked         = True
  Else
    txtJDERelease.Caption   = "JDE World (AS/400)"
    txtSMMain.Visible       = True
    txtSMWarning.Visible    = True
    ChkIsE1.Checked         = False
  End If
  '
  Screen.Refresh
  '
  txtSMMain.Text       = vArray(1)
  txtSMWarning.Text    = vArray(2)
  ChkNamedUser.Checked = (vArray(3) = "1")
  txtProcOptRel.Text   = vArray(4)
  txtPwdExp.Text       = vArray(5)
  txtPwdLen.Text       = vArray(6)
  txtRFGRel.Text       = vArray(7)
  txtJDERelease.Text   = vArray(8)
  txtJDEDB.Text        = vArray(9)
  txtLPDB.Text         = vArray(10)
  '
  ' Offline Inventory
  '
  vArray = Split(SYS.GetProperty("Config", "OI"), "|")
  '
  ChkOffline.Checked    = (vArray(0) = "1")
  ChkTranServer.Checked = (vArray(1) = "1")
  '
  ' Label Printing
  '
  vArray = Split(SYS.GetProperty("Config", "Label"), "|")
  '
  txtLPSFolder.Text     = vArray(0)
  txtLWLFolder.Text     = vArray(1)
  '
  ' Appearance
  '
  vArray = Split(SYS.GetProperty("Config", "Appear"), "|")
  '
  txtMinSearchLen.Text  = vArray(0)
  txtMaxSearchRows.Text = vArray(1)
  txtLenItem.Text       = vArray(2)
  txtLenLOCN.Text       = vArray(3)
  txtLenLOTN.Text       = vArray(4)
  txtLenQty.Text        = vArray(5)
  txtLenLP.Text         = vArray(6)


  '***7/10/09 DG: BEG ADD***
  ' Custom User Profiles

  vArray = Split(SYS.GetProperty("Profiles", "Custom"), "|")

  '
  ' Last updated
  '
  LblLastUpdate.Caption = "Last Update: " & SYS.GetProperty("Config", "LastUpd")
End Sub

Public Sub GetScreen()
  On Error Resume Next
  '
  Dim sVal As String
  '
  ' MonitorTM
  '
  sVal = Trim(txtMonitorTM.Text)     & "|" & _
         Trim(txtMonitorTMTime.Text) & "|" & _
         Trim(txtConnect.Text)
  '
  SYS.SetProperty("Config", "MonTM", sVal)
  '
  ' Environment
  '
  sVal = CStr(Abs(CInt(ChkIsE1.Checked)))      & "|" & _
         Trim(txtSMMain.Text)                  & "|" & _
         Trim(txtSMWarning.Text)               & "|" & _
         CStr(Abs(CInt(ChkNamedUser.Checked))) & "|" & _
         Trim(txtProcOptRel.Text)              & "|" & _
         Trim(txtPwdExp.Text)                  & "|" & _
         Trim(txtPwdLen.Text)                  & "|" & _
         Trim(txtRFGRel.Text)                  & "|" & _
         Trim(txtJDERelease.Text)              & "|" & _
         Trim(txtJDEDB.Text)                   & "|" & _
         Trim(txtLPDB.Text)                    & "|"

  SYS.SetProperty("Config", "Env", sVal)
  '
  ' Offline Inventory
  '
  sVal = CStr(Abs(CInt(ChkOffline.Checked)))      & "|" & _
         CStr(Abs(CInt(ChkTranServer.Checked)))   & "|"
  '
  SYS.SetProperty("Config", "OI", sVal)
  '
  ' Label Printing & License Plate
  '
  sVal = Trim(txtLPSFolder.Text)               & "|" & _
         Trim(txtLWLFolder.Text)               & "|" 
  '
  SYS.SetProperty("Config", "Label", sVal)
  '
  ' Appearance
  '
  sVal = Trim(txtMinSearchLen.Text)  & "|" & _
         Trim(txtMaxSearchRows.Text) & "|" & _
         Trim(txtLenItem.Text)       & "|" & _
         Trim(txtLenLOCN.Text)       & "|" & _
         Trim(txtLenLOTN.Text)       & "|" & _
         Trim(txtLenQty.Text)        & "|" & _
         Trim(txtLenLP.Text)         & "|"
  '
  SYS.SetProperty("Config", "Appear", sVal)
  '
  sVal = CStr(Abs(CInt(chkCustUP.Checked))) & "|"
  '
  SYS.SetProperty("Profiles", "Custom", sVal)
  '
  ' Last updated
  '
  SYS.SetProperty("Config", "LastUpd",  Trim(UCase(App.User)) & "  " & Format(Date,"MM/DD/YYYY") & " " & _
            Format(Time,"hh:nn:ss") & " " & Trim(App.IpAddress))
  LblLastUpdate.Caption  = "Last Update: " & SYS.GetProperty("Config", "LastUpd")
End Sub

