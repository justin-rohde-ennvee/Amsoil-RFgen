Private Sub txtJDEDB_OnSearch(ByRef Rsp As String, ByRef Cancel As Boolean)
  On Error Resume Next

  Dim iCnt  As Integer
  Dim sList As String

  For iCnt = 1 To 5
    If SYS.ConnectionProperty(iCnt, dcConnectMode) = "ODBC" Then App.MakeList(sList, CStr(iCnt), _
                                                    FixLeft(SYS.ConnectionProperty(iCnt,dcSourceId),10," ") & " " & _
                                                    SYS.ConnectionProperty(iCnt, dcDatabase),"Connector  Database")
  Next

  Rsp     = App.ShowList(sList, True)
  msJDEDB = SYS.ConnectionProperty(Rsp, dcDatabase)
  Rsp     = msJDEDB
  Cancel  = Len(Trim(Rsp)) = 0

End Sub

