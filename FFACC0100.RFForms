Private Sub Form_OnFkey(ByRef Fkey As Long)
  On Error Resume Next
  '
  Dim sRsp As String
  
  Dim sList As String
  '
  Select Case Fkey
    Case 5
    sList = DB.MakeList("select FANUMB as Asset, FAAPID as Unit, FAEXR as Counted from F1201 " & _
                       "where FAMCU = '%Location' and FAEXR <> ' ' order by FANUMB")
    App.ShowList(sList, True)
  End Select
End Sub

Private Sub Form_OnUpdate(ByRef Cancel As Boolean)
  On Error Resume Next
  '
  Dim emProc As New EmbeddedProc
  Dim vRsp As Variant
  '
  If (Len(App.GetValue("AssetNumber"))=0) Then
    Cancel = True
    App.MsgBox(GetMsg(5))
    App.SetFocus "txtAssetNumber"
    Exit Sub
  End If
  '
  emProc.Name                     = "TFACC0100"
  emProc.DataSource               = gsDataSource
  '
  emProc.Param("sItem")           = App.GetValue("AssetNumber")
  emProc.Param("sUser")           = App.User
  emProc.Param("sDate")           = Date
  emProc.Param("sTime")           = Time
  emProc.Param("tmUser")          = App.User
  '
  If Ext(gsLog,1) = "1" Then Call TranLog(cTNId, "1", emProc.Name, "", emProc)
  '
  If gbQueueProcessing Then
    If gbQLoadBal Then emProc.QueueName = RFQBalance("", 0)
    
    If Not emProc.Queue Then
      App.MsgBox (GetMsg(3) & vbCrLf & App.GetValue("AssetNumber"))
      Cancel = True
      App.SetFocus "txtAccept"
      Exit Sub
    End If
  Else
    If Not emProc.Execute Then
      App.MsgBox (GetMsg(3) & vbCrLf & App.GetValue("AssetNumber"))
      Cancel = True
      If Ext(gsLog,2) = "1" Then Call TranLog(cTNId, "2", emProc.Name, "", emProc)
      App.SetFocus "txtAccept"
      Exit Sub
    End If
  End If
  '
  Call ClearValues
End Sub

