Private Sub txtAssetNumber_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next
  '
  Dim sSQL  As String
  Dim sCols As String
  Dim sRows As String
  '
  If (Len(Trim(Rsp)) = 0) Then Exit Sub
  Cancel = True
  '
  ' Validate
  sSQL = "select FAAPID, FANUMB, FAASID, FADL01, FADL02, FADL03, FAEQST, FAEXR from F1201 " _
  & " where FANUMB = " & Rsp & " and FAMCU = '%Location' "
  DB.Execute(sSQL, sCols, sRows)
  '
  If (Len(sRows) = 0) Then
    App.MsgBox(GetMsg(50))
    Exit Sub
  End If
  '
  lblDesc1.Caption    = DB.Extract(sCols,sRows,1,"FADL01")
  lblDesc2.Caption    = DB.Extract(sCols,sRows,1,"FADL02")
  lblSerial.Caption   = DB.Extract(sCols,sRows,1,"FAASID")
  txtUnitNumber.Text  = DB.Extract(sCols,sRows,1,"FAAPID")
  '
  If Trim(DB.Extract(sCols,sRows,1,"FAEXR")) <> "" Then
    lblCountDate.Caption = "Counted: " & DB.Extract(sCols,sRows,1,"FAEXR")
  End If
  '
  App.SetValue("AssetNumber", Rsp)
  '
  Cancel = False
  Rsp = Trim(Rsp)
  '
  txtUnitNumber.DisplayOnly = True
End Sub

