Private Sub txtLOAD_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next
  '
  Dim sSQL     As String
  Dim sCols    As String
  Dim sRows    As String
  Dim sLPID    As String
  Dim sType    As String
  Dim sLOCN    As String
  Dim sLOCNJDE As String
  Dim sDesc    As String
  Dim nErrNo   As Long
  
  Dim vRsp     As Variant
  Dim oList    As New SearchList
  '
  Rsp = Trim(UCase(Rsp))
  '
  Cancel = True

  If Len(Rsp) > 0 Then
    '
    sSQL = "select LMTYPE, LMLOCN, LMMMCU from F55101 where LMLPID = '" & Rsp & "' and LMMCU = '%tmNMCU' "
    DB.Execute(sSQL,sCols,sRows)

    If Len(Trim(sRows)) > 0 And Trim(DB.Extract(sCols,sRows,1,1)) <> "L" Then
      App.MsgBox GetMsg(207)
      Exit Sub
    End If
    '
    If Len(sRows) = 0 Then
      vRsp = App.MsgBox(GetMsg(113), vbYesNo)
      If vRsp = vbNo Then Exit Sub
      '
      'Create new load
      '
      sDesc = cTRANDESC & " by " & Trim(UCase(App.User))
      If Not LP_CreatePlate(Rsp,"L",sDesc,App.GetValue("tmNLOCN"),False) Then Exit Sub
    Else
      txtNMCU.Text = App.GetValue("LMMCU")
    End If
  Else
    '
    'Create new load
    '
    Rsp   = "NEW"
    sDesc = cTRANDESC & " by " & Trim(UCase(App.User))
    If Not LP_CreatePlate(Rsp,"L",sDesc,App.GetValue("tmNLOCN"),False) Then Exit Sub
  End If

  App.SetValue("tmPALP", Rsp)
  Cancel = False
End Sub

