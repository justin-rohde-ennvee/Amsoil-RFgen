Private Sub Form_Load()
  On Error Resume Next
  '
  Dim sDRDL01 As String
  Dim sDRDL02 As String
  Dim sUDC    As String
  '
  Dim vArray  As Variant
  Dim iCnt    As Integer
  '
  Call SetDisplay()
  '
  ' Initialize special controls
  '
  lblPutAway.Caption = ""
  lblPutAway.Label.BackColor1 = cFieldDefaultBC
  lblPutAway.Label.ForeColor = cFieldDefaultFC
  '
  lblPick.Caption = ""
  lblPick.Label.BackColor1 = cFieldDefaultBC
  lblPick.Label.ForeColor = cFieldDefaultFC
  '
  lblRepl.Caption = ""
  lblRepl.Label.BackColor1 = cFieldDefaultBC
  lblRepl.Label.ForeColor = cFieldDefaultFC
  '
  lblSave.Caption = ""
  lblSave.Label.BackColor1 = cFieldDefaultBC
  lblSave.Label.ForeColor = cFieldDefaultFC
  '
  Call SetChkBoxes
  '
  lbxMCU.List.Clear
  vArray = Split(SYS.GetProperty("WD", "MCU"),";")
  For iCnt = 0 To UBound(vArray)
    lbxMCU.List.InsertItem(vArray(iCnt),vArray(iCnt), -1)   ' todo - upgrade: 'InsertLocn' is now first parameter
  Next iCnt
  ' Put
  txtPrioPut.Text  = SYS.GetProperty("WD", "PrioPut")
  txtZonePut.Text  = SYS.GetProperty("WD", "ZonePut")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZonePut.Text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblPutAway.Caption = sDRDL01
  ' Pick
  txtPrioPick.Text  = SYS.GetProperty("WD", "PrioPick")
  txtZonePick.Text = SYS.GetProperty("WD", "ZonePick")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZonePick.Text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblPick.Caption = sDRDL01
  txtStatusF.Text  = SYS.GetProperty("WD", "StatusF")
  txtStatusT.Text  = SYS.GetProperty("WD", "StatusT")
  ' Repl.
  txtPrioRepl.Text  = SYS.GetProperty("WD", "PrioRepl")
  txtZoneRepl.Text = SYS.GetProperty("WD", "ZoneRepl")
  sUDC = FixRight(FixLeft(Trim(UCase(txtZoneRepl.Text)), 6, " "), 10, " ")
  Validate_UDC(sUDC, "46", "ZN", sDRDL01, sDRDL02)
  lblRepl.Caption = sDRDL01
  '
  'App.SetOption ShowHorizontalScrollBar, True   ' todo - upgrade: App.SetOption removed
  'App.SetOption ShowVerticalScrollBar, True   ' todo - upgrade: App.SetOption removed
  '
End Sub

