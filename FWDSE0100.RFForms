  chkPickLMW.Checked    = (Ext(msChk, 7)  = "1")
  chkPickJDE.Checked    = (Ext(msChk, 8)  = "1")
  '
  chkRepl.Checked       = (Ext(msChk, 9)  = "1")
  chkReplLP.Checked     = (Ext(msChk,10)  = "1")
  chkReplJDE.Checked    = (Ext(msChk,11)  = "1")
  '
