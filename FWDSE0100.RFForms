Private Sub GetChkBoxes()
  On Error Resume Next
  '
  msChk = ""
  '
  msChk = Rep(msChk, 1,  -1, CStr(Abs(CInt(chkPut.Checked))))
  msChk = Rep(msChk, 2,  -1, CStr(Abs(CInt(chkPutPOLP.Checked))))
  msChk = Rep(msChk, 3,  -1, CStr(Abs(CInt(chkPutPOJDE.Checked))))
  msChk = Rep(msChk, 4,  -1, CStr(Abs(CInt(chkPutWOLP.Checked))))
  msChk = Rep(msChk, 5,  -1, CStr(Abs(CInt(chkPutWOJDE.Checked))))
  '
  msChk = Rep(msChk, 6,  -1, CStr(Abs(CInt(chkPick.Checked))))
  msChk = Rep(msChk, 7,  -1, CStr(Abs(CInt(chkPickLMW.Checked))))
  msChk = Rep(msChk, 8,  -1, CStr(Abs(CInt(chkPickJDE.Checked))))
  '
  msChk = Rep(msChk, 9, -1, CStr(Abs(CInt(chkRepl.Checked))))
  msChk = Rep(msChk, 10, -1, CStr(Abs(CInt(ChkReplLP.Checked))))
  msChk = Rep(msChk, 11, -1, CStr(Abs(CInt(chkReplJDE.Checked))))
  '
End Sub

Public Sub GetScreen()
  On Error Resume Next
  '
  Dim I       As Integer
  Dim nLine   As Integer
  Dim vArray  As Variant
  Dim iLblMax As Integer
  Dim sVal    As String
  '
  iLblMax = lbxMCU.List.Count                                           'max line
  '
  If iLblMax > 0 Then
    '
    For I = 1 To iLblMax
      lbxMCU.List.Index = I
      If Len(Trim(lbxMCU.Text)) > 0 Then
        sVal = sVal & lbxMCU.Text & ";"                                    'get current value
      End If
    Next I
  End If
  '
  SYS.SetProperty("WD", "MCU", sVal)
  '
  SYS.SetProperty("WD", "ZonePut",  txtZonePut.Text)
  SYS.SetProperty("WD", "PrioPut",  txtPrioPut.Text)
  SYS.SetProperty("WD", "ZonePick", txtZonePick.Text)
  SYS.SetProperty("WD", "PrioPick", txtPrioPick.Text)
  SYS.SetProperty("WD", "StatusF" , txtStatusF.Text)
  SYS.SetProperty("WD", "StatusT",  txtStatusT.Text)
  SYS.SetProperty("WD", "ZoneRepl", txtZoneRepl.Text)
  SYS.SetProperty("WD", "PrioRepl", txtPrioRepl.Text)
  '
  ' Last updated
  '
  SYS.SetProperty("WD", "LastUpd",  Trim(UCase(App.User)) & "  " & Format(Date,"MM/DD/YYYY") & " " & _
            Format(Time,"hh:nn:ss") & " " & Trim(App.IpAddress))
  LblSave.Caption  = "Last Update: " & SYS.getProperty("WD", "LastUpd")
End Sub
