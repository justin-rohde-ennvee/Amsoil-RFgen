Private Sub SetChkBoxes()
  On Error Resume Next
  '
  msChk = Trim(SYS.getProperty("WD", "Boxes"))
  '
  chkPut.Checked        = (Ext(msChk, 1)  = "1")
  chkPutPOLP.Checked    = (Ext(msChk, 2)  = "1")
  chkPutPOJDE.Checked   = (Ext(msChk, 3)  = "1")
  chkPutWOLP.Checked    = (Ext(msChk, 4)  = "1")
  chkPutWOJDE.Checked   = (Ext(msChk, 5)  = "1")
  '
