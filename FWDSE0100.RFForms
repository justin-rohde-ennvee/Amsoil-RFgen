Private Sub txtMCU_OnEnter(ByRef Rsp As String, ByRef Cancel As Boolean, ByRef ErrMsg As String)
  On Error Resume Next
  '
  Dim sCompany As String
  Dim sMCU     As String
  Dim sVal     As String
  Dim sDsc     As String
  Dim sDsp     As String
  '
  Dim I       As Integer
  Dim nLine   As Integer
  Dim vArray  As Variant
  Dim iLblMax As Integer
  Dim bDel    As Boolean
  '
  If (Len(Trim(Rsp))=0) Then Exit Sub
  Cancel = True
  '
  If Not Validate_BranchPlant(Rsp, sMCU, sCompany, sDsc) Then Exit Sub
  '
  App.SetValue("tmCO",   sCompany)
  App.SetValue("tmMCU",  sMCU)
  '
  iLblMax = lbxMCU.List.Count                                           'max line
  bDel = False
  sDsp = sMCU & "|" & sDsc
  '
  If iLblMax = 0 Then
      lbxMCU.List.InsertItem(sDsp,sDsp, -1)   ' todo - upgrade: 'InsertLocn' is now first parameter
      Exit Sub
  End If
  '
  For I = 1 To iLblMax
    lbxMCU.List.Index = I
    sVal = lbxMCU.Text                                                 'get current value
    vArray = Split(sVal,"|")
    If Trim(vArray(0)) = Trim(Rsp) Then
      lbxMCU.List.RemoveItem(I)                  'remove line
      bDel = True
      Exit For
    End If
    Next I
  '
    If Not bDel Then lbxMCU.List.InsertItem(sDsp,sDsp, -1)   ' todo - upgrade: 'InsertLocn' is now first parameter
    lbxMCU.List.Sorted = True
    Screen.Refresh
    '
End Sub

