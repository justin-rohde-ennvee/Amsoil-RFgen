Option Explicit
Private sSql    As String
Private sCols   As String
Private sRows   As String
Private msChk   As String
'
Private Sub btnSave_Click()
  On Error Resume Next
  '
  Call GetChkBoxes
  SYS.SetProperty("WD", "Boxes", msChk)
  Call GetScreen
  '
End Sub

Private Sub chkPutPOJDE_Click()
  On Error Resume Next
  '
  If chkPutPOJDE.Checked = True Then
    chkPutPOLP.Checked = False
  Else
    chkPutPOLP.Checked = True
  End If
  '
  Screen.Refresh
  '
End Sub

Private Sub chkPutPOLP_Click()
  On Error Resume Next
  '
  If chkPutPOLP.Checked = True Then
    chkPutPOJDE.Checked = False
  Else
    chkPutPOJDE.Checked = True
  End If
